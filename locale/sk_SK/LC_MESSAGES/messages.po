msgid "UTF-8 encoding"
msgstr "UTF-8 kódovanie"

#: src/template/htaccess.txt:432
msgid "use utf-8 encoding for anything served text/plain or text/html"
msgstr "použite kódovanie utf-8 na čokoľvek čo slúži ako text/plain alebo text/html"

